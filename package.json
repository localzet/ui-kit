{
  "name": "@localzet/ui-kit",
  "version": "1.0.0",
  "description": "UI Kit библиотека компонентов на основе Mantine для проектов Localzet",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css",
    "./theme": {
      "import": "./dist/theme/index.js",
      "types": "./dist/theme/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ui-kit",
    "mantine",
    "react",
    "components",
    "localzet"
  ],
  "author": {
    "name": "localzet",
    "email": "creator@localzet.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/localzet/ui-kit"
  },
  "peerDependencies": {
    "@mantine/core": "^8.3.6",
    "@mantine/hooks": "^8.3.6",
    "@mantine/charts": "^8.3.6",
    "@mantine/dates": "^8.3.6",
    "@mantine/nprogress": "^8.3.6",
    "react": "^18.2.0 || ^19.0.0",
    "react-dom": "^18.2.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@mantine/charts": {
      "optional": true
    },
    "@mantine/dates": {
      "optional": true
    }
  },
  "dependencies": {
    "@tabler/icons-react": "^3.35.0",
    "clsx": "^2.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@mantine/core": "^8.3.6",
    "@mantine/hooks": "^8.3.6",
    "@mantine/charts": "^8.3.6",
    "@mantine/dates": "^8.3.6",
    "@types/node": "^22.15.30",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^5.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.3.0"
  }
}

